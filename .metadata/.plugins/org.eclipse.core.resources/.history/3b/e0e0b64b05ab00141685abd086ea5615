
public class EllipsisExample {

	/**
	 * @param args
	 */
	public static void main(String[] args) {
		// TODO Auto-generated method stub

	}

	public String[] generalConcatAll(String...str) {

	    StringBuilder sb = new StringBuilder();

	    for(String[] array : str) {
	         sb.append(array);
	    }

	    return sb.toString();

	}
}
